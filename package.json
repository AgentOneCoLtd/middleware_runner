{
  "name": "@ag1/middleware_runner",
  "version": "0.2.0",
  "description": "mimic express middleware execution for testing",
  "main": "./lib/index.js",
  "jsnext:main": "./lib-module/index.js",
  "module": "./lib-module/index.js",
  "typings": "./lib/index.d.ts",
  "author": "agent one co., ltd.",
  "license": "MIT",
  "private": false,
  "scripts": {
    "tsc:lib": "tsc -b tsconfig.json",
    "tsc:lib-module": "tsc -b tsconfig-module.json",
    "compile": "yarn tsc:lib && yarn tsc:lib-module",
    "lint:onlyts": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:spects": "tslint -c tslint-spects.json -p tsconfig-spects.json -t stylish -e \"**/!(*.spec).ts\"",
    "lint": "yarn lint:onlyts --fix && yarn lint:spects --fix",
    "prettier:config": "prettier --no-config --write \"./*.{json,yml}\"",
    "prettier:ts": "prettier --write \"./{src,test}/**/*.ts\"",
    "prettier": "yarn prettier:config && yarn prettier:ts",
    "test": "jest --config \"./jest.config.json\"",
    "build": "yarn prettier && yarn lint && yarn compile && yarn test"
  },
  "dependencies": {
    "@ag1/nil": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "express": "^4.16.4",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "rxjs": "^6.4.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.3333"
  },
  "keywords": [
    "express",
    "middleware",
    "test",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/AgentOneCoLtd/middleware_runner.git",
    "type": "git"
  },
  "files": [
    "lib",
    "lib-module",
    "src",
    "test"
  ]
}
